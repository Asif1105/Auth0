{
    "name": "shop-smart",
    "version": "1.0.0",
    "license": "MIT",
    "repository": {
        "url": "https://git.epam.com/asif_buhari/eComm.git",
        "type": "git"
    },
    "keywords": [
        "eComm",
        "shop"
    ],
    "engines": {
        "node": ">= 10.13.0"
    },
    "browser": {
        "src": false
    },
    "scripts": {
        "start": "grunt dev --base ./ --gruntfile ./scripts/start.js",
        "build": "grunt prod --base ./ --gruntfile ./scripts/start.js",
        "serve": "json-server --watch ./packages/core/database/shop.json",
        "verify": "yarn lint && yarn test",
        "test": "jest --detectOpenHandles --no-cache --silent",
        "lint": "eslint \"./**/*.{js,jsx,ts,tsx}\" --fix",
        "format": "yarn lint",
        "commit": "git-cz -n  --no-verify",
        "prepush": "yarn verify",
        "clean": "rimraf node_modules",
        "clean:install": "yarn clean && yarn install"
    },
    "config": {
        "commitizen": {
            "path": "./scripts/commits.js"
        }
    },
